function saveExam(event) {
    event.preventDefault();
    const formData = new FormData(event.target);
    const examData = Object.fromEntries(formData);
    
    const url = examData.id ? `/exams/${examData.id}` : '/exams';
    
    if (!examData.id) {
        formData.append('_token', document.querySelector('meta[name="csrf-token"]').getAttribute('content'));
    } else {
        formData.append('_method', 'PUT');
        formData.append('_token', document.querySelector('meta[name="csrf-token"]').getAttribute('content'));
    }
    
    fetch(url, {
        method: 'POST',
        body: formData,
        headers: {
            'X-Requested-With': 'XMLHttpRequest'
        }
    })
    .then(response => response.json())
    .then(data => {
        if (data.success) {
            if (examData.id) {
                const index = exams.findIndex(e => e.id == examData.id);
                if (index !== -1) {
                    exams[index] = data.exam;
                }
            } else {
                exams.push(data.exam);
            }
            
            filteredExams = [...exams];
            renderExamsTable();
            updateStats();
            closeModal();
            showAlert('‡¶∏‡¶´‡¶≤!', '‡¶™‡¶∞‡ßÄ‡¶ï‡ßç‡¶∑‡¶æ ‡¶∏‡¶´‡¶≤‡¶≠‡¶æ‡¶¨‡ßá ‡¶∏‡¶Ç‡¶∞‡¶ï‡ßç‡¶∑‡¶£ ‡¶ï‡¶∞‡¶æ ‡¶π‡¶Ø‡¶º‡ßá‡¶õ‡ßá', 'success');
